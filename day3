function isValidNumber(num) {

  if(isNaN(num)){
      console.log(`${num} is not a valid number`)
  }
  else{
    console.log(`${num} is a valid number`)
  }
}

isValidNumber("21") //"21 is a valid number"
isValidNumber("vhvhv") //"vhvhv is a valid number"
isValidNumber("xyzscasc") //"xyzcasc is not a number"
isValidNumber("17.5") //"17.5 is a valid number"
isValidNumber("21xadxasF") //"21xadxasF is not a number"
